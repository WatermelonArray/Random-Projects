{
  "name": "RPG",
  "tree": {
    "$className": "DataModel",

    "HttpService": {
      "$className": "HttpService",
      "$properties": {
        "HttpEnabled": true
      }
    },

    "Lighting": {
      "$className": "Lighting",
      "$properties": {
        "Brightness": 2.0,
        "Technology": "ShadowMap",
        "Ambient": [
          0.0,
          0.0,
          0.0
        ],
        "GlobalShadows": true,
        "Outlines": false
      }
    },

    "StarterPlayer": {
      "$className": "StarterPlayer",

      "StarterPlayerScripts": {
        "$className": "StarterPlayerScripts",
        "client": {
          "$path": "client"
        }
      }
    },

    "ServerScriptService": {
      "$className": "ServerScriptService",

      "server": {
        "$path": "server"
      }
    }, 

    "ReplicatedStorage": {
      "$className": "ReplicatedStorage",
      "source": {
        "$path": "src"
      }
    },

    "SoundService": {
      "$className": "SoundService",
      "$properties": {
        "RespectFilteringEnabled": true
      }
    },

    "Workspace": {
      "$className": "Workspace",
      "$properties": {
        "FilteringEnabled": true
      }
    }
  }
}